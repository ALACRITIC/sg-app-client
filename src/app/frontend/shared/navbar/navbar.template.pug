nav.navbar.navbar-static-top.navbar-default.navbar-full
    .navbar-inner
        .container
            .row
                .col-md-6
                    .navbar-header
                        button.navbar-toggler.hidden-lg-up( (click)="isCollapsed = !isCollapsed" type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1')
                            span.sr-only Toggle navigation
                            i.fa.fa-bars
                        a(routerLink='/')
                            img(alt="Brand" src='/assets/img/New_SG_Logo.png')
                            | &nbsp; &nbsp; &nbsp; STUDENT GOVERNMENT
                .col-md-6

                    .collapse.navbar-collapse.navbar-toggleable-md(   id="bs-example-navbar-collapse-1")
                        ul.nav.navbar-nav
                            li.nav-item(routerLinkActive='active', *ngFor='let route of navbarRoutes')
                                a.nav-link(routerLink='{{route.url}}') {{route.name}}
